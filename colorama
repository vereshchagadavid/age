import colorama
import inspect


print("=== dir(colorama) ===")
print(dir(colorama))


print("\n=== Documentation ===")
print(inspect.getdoc(colorama))


print("\n=== Fore Colors ===")
print(dir(colorama.Fore))

print("\n=== Back Colors ===")
print(dir(colorama.Back))

print("\n=== Styles ===")
print(dir(colorama.Style))
